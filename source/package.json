{
  "name": "workout-app",
  "version": "1.0.0",
  "description": "Gym Workout Tracker mit React, MUI, Electron und SQLite",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "build": "webpack --config webpack.config.js",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "Dein Name",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/material": "^5.13.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "electron": "^28.2.10",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "com.example.workoutapp",
    "win": {
      "target": "portable"
    },
    "extraFiles": [
      "data/workouts.db"
    ]
  }
}
